  <link rel="stylesheet" type="text/css" href="assets/materialize.min.css">
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.css"> 
  <style>
    /* fallback */
    @font-face {
      font-family: 'Material Icons';
      font-style: normal;
